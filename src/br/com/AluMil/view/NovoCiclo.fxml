<AnchorPane id="AnchorPane" fx:id="apPrincipal" prefHeight="210.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65" fx:controller="br.com.AluMil.view.NovoCicloController">
